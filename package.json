{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.32.1",
    "@umijs/preset-react": "2.x",
    "antd": "^4.18.2",
    "axios": "^0.24.0",
    "js-cookie": "^3.0.1",
    "query-string": "^7.0.1",
    "react-markdown": "^7.1.1",
    "remark-gfm": "^3.0.1",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@umijs/test": "^3.5.20",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.5.4",
    "yorkie": "^2.0.0"
  }
}
