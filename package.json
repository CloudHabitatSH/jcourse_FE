{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.1.9",
    "@umijs/preset-react": "2.x",
    "ahooks": "^3.1.9",
    "antd": "^4.18.6",
    "axios": "^0.25.0",
    "react-markdown": "^8.0.0",
    "react-responsive": "^9.0.0-beta.6",
    "remark-gfm": "^3.0.1",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@umijs/test": "^3.5.20",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.5.5",
    "yorkie": "^2.0.0"
  }
}
